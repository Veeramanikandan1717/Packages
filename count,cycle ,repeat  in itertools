from itertools import cycle,count,repeat

#count(infinite iterators)
for i in count(50):
    print(i)
    if i==100:
        break

#cycle
a=[1,3,4,4]
for i in cycle(a):
    print(i)

#repeat
for i in repeat("kannan",5):  #arg 1 :repeat ele ,arg2 :count of repatition
    print(i)
for i in repeat("5",5):
    print(i)
